onse = await api.spotify.users.getUserItems();\n      this.artists = response.data.items;\n    },\n    getArtistId(artist) {\n      this.id = artist.id;\n    },\n  },\n  components: {\n    Albums,\n  },\n};\n</script>\n\n<style scoped>\nimg {\n  object-fit: cover;\n}\n</style>\n"]}]}